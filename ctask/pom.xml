                  <target>1.6</target>
                  <encoding>utf-8</encoding>
                  <!--解决com.sun.image.codec.jpeg.JPEGCodec 是 Sun 的专用 API，可能会在未来版本中删除-->
                  <compilerArguments>
                      <verbose />
                      <bootclasspath>${java.home}\lib\rt.jar</bootclasspath>
                  </compilerArguments>
              </configuration>
          </plugin>
          <plugin>
              <artifactId>maven-resources-plugin</artifactId>
              <version>2.2</version>
              <configuration>
                  <encoding>utf-8</encoding>
              </configuration>
          </plugin>
      </plugins>
  </build>
