  <build>
    <finalName>ctask</finalName>
      <resources>
          <resource>
              <directory>src/main/resources</directory>
              <filtering>true</filtering>
          </resource>
      </resources>
      <plugins>
          <plugin>
              <groupId>org.apache.maven.plugins</groupId>
              <artifactId>maven-compiler-plugin</artifactId>
              <configuration>
                  <source>1.6</source>
                  <target>1.6</target>
                  <encoding>utf-8</encoding>
                  <!--解决com.sun.image.codec.jpeg.JPEGCodec 是 Sun 的专用 API，可能会在未来版本中删除-->
                  <compilerArguments>
                      <verbose />
                      <bootclasspath>${java.home}\lib\rt.jar</bootclasspath>
                  </compilerArguments>
              </configuration>
          </plugin>
          <plugin>
              <artifactId>maven-resources-plugin</artifactId>
              <version>2.2</version>
              <configuration>
                  <encoding>utf-8</encoding>
              </configuration>
          </plugin>
      </plugins>
  </build>
    <profiles>
        <profile>
            <!-- 测试环境的profile -->
            <id>development</id>
            <properties>
                <cjtask.read.jdbc.driver>com.mysql.jdbc.Driver</cjtask.read.jdbc.driver>
                <cjtask.read.jdbc.url></cjtask.read.jdbc.url>
                <cjtask.read.jdbc.username></cjtask.read.jdbc.username>
                <cjtask.read.jdbc.password>123456</cjtask.read.jdbc.password>
                <com.jc.cjtask.httpClientUtil.charSet>utf-8</com.jc.cjtask.httpClientUtil.charSet>
                <com.jc.cjtask.delPath>html</com.jc.cjtask.delPath>
                <!--=================== task cronExpression 开始 ===================-->
                <com.jc.deltePathFilesTrigger.cronExpression>0 * * * * ?</com.jc.deltePathFilesTrigger.cronExpression>
                <!--=================== task cronExpression 结束 ===================-->
                <com.jc.stratUp.ip1></com.jc.stratUp.ip1>
            </properties>
        </profile>
        <profile>
            <!-- 测试环境的profile -->
            <id>production</id>
            <properties>
                <cjtask.read.jdbc.driver>com.mysql.jdbc.Driver</cjtask.read.jdbc.driver>
                <cjtask.read.jdbc.url>j</cjtask.read.jdbc.url>
                <cjtask.read.jdbc.username></cjtask.read.jdbc.username>
                <cjtask.read.jdbc.password></cjtask.read.jdbc.password>
                <com.jc.cjtask.httpClientUtil.charSet>utf-8</com.jc.cjtask.httpClientUtil.charSet>
                <com.jc.cjtask.delPath>html</com.jc.cjtask.delPath>
                <!--=================== task cronExpression 开始 ===================-->
                <com.jc.deltePathFilesTrigger.cronExpression>0 * * * * ?</com.jc.deltePathFilesTrigger.cronExpression>
                <!--=================== task cronExpression 结束 ===================-->
                <com.jc.stratUp.ip1></com.jc.stratUp.ip1>
            </properties>
        </profile>
    </profiles>
